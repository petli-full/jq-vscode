{
	"publisher": "petli-full",
	"name": "jq-vscode",
	"displayName": "jq-vscode",
	"description": "an inline jq processer for vscode",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.45.0"
	},
	"repository": "https://github.com/petli-full/jq-vscode",
	"license": "MIT",
	"icon": "images/favicon.png",
	"galleryBanner": {
		"color": "#F2F2F2",
		"theme": "light"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:jq-vscode.openjq",
		"onLanguage:json",
		"onLanguage:jqx"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "jq-vscode.openjq",
				"title": "Open a new file to exec jq",
				"icon": "$(new-file)",
				"category": "JQ"
			}
		],
		"languages": [
			{
				"id": "jqx",
				"aliases": [
					"JQX",
					"jqx"
				],
				"extensions": [
					".jqx"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "jqx",
				"scopeName": "source.jqx",
				"path": "./syntaxes/jqx.tmLanguage.json"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "jq-vscode.openjq",
					"when": "editorLangId == json",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"generate": "node ./scripts/generate-language.js",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"ts-loader": "^7.0.4",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"jqdash": "^1.0.1",
		"rxjs": "^6.5.5"
	}
}